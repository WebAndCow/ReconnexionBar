{
    "name": "barbierp35/reconnexion-bar",
    "description": "ReconnexionBar plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "ReconnexionBar\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ReconnexionBar\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
